				},
				"column2":{
					"type":"enum",
					"length":"'YES','NO'",
					"default":"YES",
					"comment":"ENUM 타입"
				},
				"column3":{
					"type":"text",
					"is_null":false,
					"comment":"TEXT 타입"
				},
				"column4":{
					"type":"date",
					"default":"0000-00-00",
					"comment":"날짜(YYYY-MM-DD) 타입"
				}
			},
			"indexes":{
				"column1,column2":"primary_key"
			}
		}
	}
